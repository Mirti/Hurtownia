<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="180.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hurtownia.UserController">
        <children>
       <Label layoutX="25.0" layoutY="19.0" text="Użytkownicy">
          <font>
             <Font size="16.0" />
          </font>
       </Label>

       <Button layoutX="25.0" layoutY="333.0" mnemonicParsing="false" onAction="#searchUser" prefHeight="64.0" prefWidth="191.0" text="Pokaż wszystkich">
          <font>
             <Font size="18.0" />
          </font>
       </Button>

       <ScrollPane layoutX="25.0" layoutY="44.0" prefHeight="280.0" prefWidth="722.0">
          <content>
             <TableView fx:id="UserTable" prefHeight="285.0" prefWidth="723.0">
                <columns>
                   <TableColumn fx:id="usrFirstNameColumn" prefWidth="75.0" text="Imie" />
                   <TableColumn fx:id="usrLastNameColumn" prefWidth="75.0" text="Nazwisko" />
                   <TableColumn fx:id="usrPermissionsColumn" prefWidth="75.0" text="Stanowisko" />
                   <TableColumn fx:id="usrLoginColumn" prefWidth="75.0" text="Login" />
                   <TableColumn prefWidth="75.0" text="Edytuj / Usuń" />
                </columns>
                <columnResizePolicy>
                   <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                </columnResizePolicy>
             </TableView>
          </content>
       </ScrollPane>
      <Button layoutX="566.0" layoutY="333.0" mnemonicParsing="false" onAction="#openEdycjaDodawanieUsera" prefHeight="64.0" prefWidth="191.0" text="Dodaj">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
    </children></AnchorPane>
